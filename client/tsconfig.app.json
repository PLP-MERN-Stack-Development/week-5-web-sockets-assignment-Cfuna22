{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/": [
                "src/*"
            ]
        },
        "forceConsistentCasingInFileNames": true,
        "strict": true
    },
    "include": [
        "src/**/*.ts",
        "src/**/*.tsx",
        "src/**/*.js",
        "src/**/*.jsx"
    ]
}